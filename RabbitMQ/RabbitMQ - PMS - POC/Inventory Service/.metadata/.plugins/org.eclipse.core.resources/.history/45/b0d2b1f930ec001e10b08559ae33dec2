package io.spring.rabbitmq.rmqinventoryservice.message;

public class InventoryResponseMessage {

	private String messageId;
	private String message;
	private String orderId;
	private boolean isFullilable;

	public InventoryResponseMessage() {
		super();
		// TODO Auto-generated constructor stub
	}

	public InventoryResponseMessage(String messageId, String message, String skuCode, int quantity,
			boolean isFullilable) {
		super();
		this.messageId = messageId;
		this.message = message;
		this.skuCode = skuCode;
		this.quantity = quantity;
		this.isFullilable = isFullilable;
	}

	public String getMessageId() {
		return messageId;
	}

	public void setMessageId(String messageId) {
		this.messageId = messageId;
	}

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}

	public String getSkuCode() {
		return skuCode;
	}

	public void setSkuCode(String skuCode) {
		this.skuCode = skuCode;
	}

	public int getQuantity() {
		return quantity;
	}

	public void setQuantity(int quantity) {
		this.quantity = quantity;
	}

	public boolean isFullilable() {
		return isFullilable;
	}

	public void setFullilable(boolean isFullilable) {
		this.isFullilable = isFullilable;
	}

	@Override
	public String toString() {
		return "InventoryResponseMessage [messageId=" + messageId + ", message=" + message + ", isFullilable="
				+ isFullilable + "]";
	}

}
